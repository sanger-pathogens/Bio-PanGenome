#!/usr/bin/env perl
use Moose;
use Getopt::Long;
my($output_filename,$dbtype,$in,$logfile);
GetOptions(
    'o|out=s' => \$output_filename,
    'd|dbtype=s' => \$dbtype,
    'l|logfile=s' => \$logfile,
    'i|in=s' => \$in,
);

system("touch $output_filename.phr");
system("touch $output_filename.pin");
system("touch $output_filename.psq");

1;